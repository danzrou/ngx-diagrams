{
	"name": "ngx-diagrams-workspace",
	"version": "3.3.2",
	"author": {
		"email": "daniel.netzer7@gmail.com",
		"name": "Daniel Netzer"
	},
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build --base-href /ngx-diagrams/ --prod",
		"build-lib": "ng build ngx-diagrams --prod",
		"test": "ng test",
		"lint": "ng lint",
		"lint-lib": "ng lint ngx-diagrams",
		"e2e": "ng e2e",
		"pack-zip": "cd dist/ngx-diagrams && npx bestzip ../ngx-diagrams.zip * && cd ../../",
		"semantic-release": "npx semantic-release",
		"build-storybook": "npm run docs:json && build-storybook",
		"snyk-protect": "snyk protect",
		"prepare": "npm run snyk-protect",
		"docs:json": "compodoc -p ./projects/ngx-diagrams/tsconfig.lib.prod.json -e json -d .",
		"storybook": "npm run docs:json && start-storybook -p 6006"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DanielNetzer/ngx-diagrams.git"
	},
	"dependencies": {
		"@angular/animations": "^11.0.1",
		"@angular/common": "^11.0.1",
		"@angular/core": "^11.0.1",
		"@angular/forms": "^11.0.1",
		"@angular/platform-browser": "^11.0.1",
		"@angular/platform-browser-dynamic": "^11.0.1",
		"@angular/router": "^11.0.1",
		"core-js": "^3.6.5",
		"dagre": "^0.8.5",
		"easystarjs": "^0.4.4",
		"pathfinding": "0.4.18",
		"paths-js": "0.4.11",
		"rxjs": "~6.6.3",
		"snyk": "^1.420.0",
		"tslib": "^2.0.3",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1100.2",
		"@angular-devkit/build-ng-packagr": "~0.1000.2",
		"@angular/cli": "~11.0.2",
		"@angular/compiler": "~11.0.1",
		"@angular/compiler-cli": "~11.0.1",
		"@angular/language-service": "~11.0.1",
		"@babel/core": "^7.12.3",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@compodoc/compodoc": "^1.1.11",
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/exec": "^5.0.0",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/npm": "^7.0.8",
		"@types/pathfinding": "^0.0.4",
		"@storybook/addon-actions": "^6.1.0",
		"@storybook/addon-docs": "^6.1.0",
		"@storybook/addon-essentials": "^6.1.0",
		"@storybook/addon-links": "^6.1.0",
		"@storybook/angular": "^6.1.0",
		"@types/dagre": "^0.7.44",
		"@types/jasmine": "~3.6.1",
		"@types/jasminewd2": "~2.0.6",
		"@types/node": "^14.14.8",
		"babel-loader": "^8.0.5",
		"codelyzer": "^6.0.1",
		"cz-customizable": "^6.3.0",
		"git-cz": "^4.7.4",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.2",
		"ng-packagr": "^11.0.2",
		"prettier": "^2.1.2",
		"protractor": "~7.0.0",
		"react-is": "^16.13.1",
		"semantic-release": "^17.2.1",
